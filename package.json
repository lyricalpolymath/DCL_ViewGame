{
  "name": "leveler-game-jam-2019",
  "version": "1.0.0",
  "description": "Collaborative Game Jam 2019",
  "scripts": {
    "start": "dcl start",
    "build": "build-ecs",
    "watch": "build-ecs --watch",
    "deploy:now": "dcl export && now export",
    "ecs:install": "npm install --save-dev decentraland-ecs@latest",
    "ecs:install-next": "npm install --save-dev decentraland-ecs@next",
    "reset": "rm -r package-lock.json ./bin ./node_modules & npm install"
  },
  "devDependencies": {
    "decentraland-ecs": "^6.4.2"
  },
  "dependencies": {
    "decentraland-ecs-utils": "latest",
    "now": "latest"
  }
}
